h5 = "Tabela sezonu #{Training::Activities::Repository.new.start_date.year}/#{Training::Activities::Repository.new.end_date.year}"
table.stack
  thead
    tr
      th width='5%' Miejsce
      th width='50%' Kto?
      th width='45%' Przewy≈ºszenie

  tbody
    - i = 0
    - routes.each do |user|
      tr
        td.text-center = i + 1
        td.text-center.large-text-left = Db::User.find(user.id).display_name
        td.text-center.large-text-left = user.sum
      - i += 1
