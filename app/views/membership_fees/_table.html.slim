table.stack
  thead
    tr
      th
        | Rok
      th
        | Reaktywacja
      th
        | Opłacono
  tbody
    - fees.each do |fee|
      tr
        td
          = fee.year
        td
          = fee.reactivation? ? "Tak" : "Nie"
        td
          = fee.order && fee.order.payment.prepaid? ? "Zapłacone" : "Nie zapłacone"
